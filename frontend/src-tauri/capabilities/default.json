{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://localhost:3003/v1/*" },
        { "url": "https://valorant-api.com/v1/*" }
      ]
    }
  ]
}
